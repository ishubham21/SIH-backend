{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint",
    "no-loops",
    "import"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-useless-escape": "off",
    "no-undef": "off",
    "no-console": "warn",
    "no-alert": "error",
    "camelcase": "error",
    "no-loops/no-loops": "error",
    "@typescript-eslint/no-inferrable-types": "off"
  },
  "env": {
    "node": true,
    "es6": true
  }
}